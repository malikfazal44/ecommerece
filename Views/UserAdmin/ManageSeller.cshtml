@model IList<Seller>

    <h1>
        Number of Total Sellers: @Model.Count
    </h1>
    <table class="table">
    <thead>
        <tr>
            <th> Action </th>
            <th> ID </th>
            <th> Name </th>
            <th> Email </th>
            <th> CNIC </th>
            <th> Mobile Number </th>
            <th> Address </th>
            <th> Company Name </th>
            <th> Website URL </th>
            <th> Gender </th>
        </tr>


    </thead>
    <tbody>
        @foreach (Seller O in Model)
        {
            <tr>
                <td>
                    <a asp-controller="" asp-action="" class="btn btn-primary btn-sm"> View </a>
                    <a asp-controller="" asp-action="" class="btn btn-success btn-sm"> Update </a>
                    <a asp-controller="UserAdmin" asp-action="DeleteSeller" asp-route-id="@O.SellerId" class="btn btn-danger btn-sm"> Delete </a>
                </td>
                <td>
                    @O.SellerId
                </td>
                <td>
                    @O.SellerName
                </td>
                <td>
                    @O.SellerEmail
                </td>
                <td>
                    @O.SellerCnic
                </td>
                <td>
                    @O.SellerPhone
                </td>
                <td>
                    @O.SellerAddress
                </td>
                <td>
                    @O.CompanyName
                </td>
                <td>
                    @O.WebsiteUrl
                </td>
                <td>
                    @O.SellerGender
                </td>
            </tr>
        }
    </tbody>
    </table>